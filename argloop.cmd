@if [%CKP%]==[] echo off
setlocal
prompt %~n0$G$G$S
:CHECK_QUAL_LOOP
    if %1.==. goto :CHECK_DONE
    echo arg is %1
    shift /1
    goto :CHECK_QUAL_LOOP
:CHECK_DONE
:BYE
echo Command line args are %*
endlocal
goto :EOF
